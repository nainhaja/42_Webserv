Server {
    listen             127.0.0.1 8083
    root             ./HTTP
    allow_methods      GET POST 

    upload_path        ./HTTP/PATH03/
    
    server_name       example.com

    index index.html
    error_page      404 ./HTTP/error_pages/404.html
    error_page      403 ./HTTP/error_pages/403.html
    error_page      400 ./HTTP/error_pages/400.html
    error_page      405 ./HTTP/error_pages/405.html
    error_page      413 ./HTTP/error_pages/413.html
    error_page      500 ./HTTP/error_pages/500.html
    error_page      504 ./HTTP/error_pages/504.html
    autoindex on

    
    redirection    youtube   www.youtube.com
    redirection    /ok 3yqti

    
    location {
        root ./HTTP
        path /PATH03/res.mp4
        autoindex on
        allow_methods  POST GET
    }
    
    location {
        root ./HTTP
        path /PATH03/res.png  
        autoindex off
        allow_methods  DELETE GET POST
    }
    location {
        root ./HTTP
        path /
        autoindex on
        allow_methods DELETE POST GET
    }

    location {
        root ./HTTP
        path /PATH03/index.html
        autoindex on
        allow_methods DELETE POST GET
    }

    cgi .py
    {
        allow_methods      POST
        cgi_path         /usr/local/bin/python3
    }

}

Server {
    listen             127.0.0.1 8084
    root             ./HTTP
    allow_methods      GET POST 

    upload_path        /somewhere/ 

    index index.html index.php 
     
    error_page      404 ./HTTP/error_pages/404.html
    error_page      403 ./HTTP/error_pages/403.html
    error_page      405 ./HTTP/error_pages/405.html
    error_page      400 ./HTTP/error_pages/400.html
    error_page      500 ./HTTP/error_pages/500.html
    error_page      504 ./HTTP/error_pages/504.html
    error_page      413 ./HTTP/error_pages/413.html
    autoindex off

    redirection    /doooooxsxvasvf /test/
    
    server_name       nizar.com


    location {
        root ./HTTP
        path /PATH03/ayoub.html
        autoindex on
        allow_methods  GET POST
    }
    
    location {
        root ./HTTP
        path /PATH03/
        autoindex on
        allow_methods GET  POST
    } 


    location {
        root ./HTTP
        path /PATH03/todelete
        autoindex on
        allow_methods GET DELETE
    } 
    cgi .php
    {
        allow_methods      POST
        cgi_path         /Volumes/SPYDERSY/webserv/cgi-test/script.py
    }

    client_max_body_size 4343
}